<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<application xmlns="http://wadl.dev.java.net/2009/02"
	xmlns:cs="http://www.xsd.transport.androidoven.com/CookService"
	xmlns:cm="http://www.xsd.transport.androidoven.com/Common">

	<grammars>
		<include href="../xsd/CookService.xsd" />
		<include href="../xsd/Common.xsd" />
	</grammars>

	<resources base="http://www.androidoven.com/dnd/api">
		<resource id="com.androidoven.transport.wadl.CookResource">
			<resource path="/signinCook">
				<method id="signinCook" name="POST">
					<request>
						<representation mediaType="application/json" element="cm:Cook"/>
					</request>
					<response>
						<representation mediaType="application/json" element="cm:Cook"/>
					</response>
				</method>
			</resource>
		</resource>
	</resources>

</application>